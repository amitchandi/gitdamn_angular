  "bugs": {
    "url": "https://github.com/amitchandi/gitdamn_angular/issues"
  },
  "homepage": "https://github.com/amitchandi/gitdamn_angular#readme",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}

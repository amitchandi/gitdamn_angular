{
  "name": "gitdamn_angular",
  "version": "1.0.0",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:all": "npm run install:frontend && npm run install:backend",
    "build:frontend": "cd frontend && ng build --prod",
    "start:frontend": "cd frontend && ng serve --configuration=development",
    "start:backend": "cd backend && node ./dist/index.js",
    "start": "concurrently -k -n FRONT,BACKEND -c \"blue,green\" \"npm run start:frontend\" \"npm run start:backend\"",
    "deploy": "npm run install:all && npm run build:frontend && npm run start:backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amitchandi/gitdamn_angular.git"
  },
